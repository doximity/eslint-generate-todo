{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["./src/**/*"],
  "ts-node": {
    "esm": true
  }
}
